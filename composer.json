{
    "name": "maxime-rainville/silverstripe-react",
    "description": "A helper library for creating react component in Silverstripe CMS",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/framework": "^4.8",
        "silverstripe/admin": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "MaximeRainville\\SilverstripeReact\\": "src/",
            "MaximeRainville\\SilverstripeReact\\Tests\\": "tests/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev"
}
